{
  "name": "Mofn",
  "private": false,
  "description": "This was made in the mofn.",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OmgImAlexis/Mofn.git"
  },
  "bugs": {
    "url": "https://github.com/OmgImAlexis/Mofn/issues"
  },
  "homepage": "https://github.com/OmgImAlexis/Mofn#readme",
  "scripts": {
    "start": "NODE_ENV=production && node index.js",
    "start-dev": "NODE_ENV=develop && node index.js",
    "test-js": "xo"
  },
  "dependencies": {
    "command-line-usage": "^4.0.0",
    "config": "^1.25.1",
    "express": "^4.14.1",
    "git-rev": "^0.2.1",
    "git-rev-sync": "^1.8.0",
    "junk": "^2.0.0",
    "scene-release": "^1.4.0",
    "webworker-threads": "^0.7.11",
    "winston": "^2.3.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "stylelint": "^7.2.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 4,
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "envs": [
      "node",
      "es6"
    ],
    "globals": [
      "log",
      "api"
    ],
    "ignores": [
      "**/*.min.js"
    ]
  },
  "stylelint": {
    "extends": "./.build/node_modules/stylelint-config-standard",
    "rules": {
      "indentation": [
        4,
        {
          "ignore": [
            "block"
          ],
          "message": "Please use 4 spaces for indentation. Tabs make OmgImAlexis sad.",
          "severity": "error"
        }
      ]
    }
  }
}
